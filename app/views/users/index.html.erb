<header>
  <nav>
    <% if user_signed_in? %>
    <%= link_to 'プロフィール変更', edit_user_registration_path %>
    <%= link_to '書籍一覧', books_path %>
    <%= link_to 'ログアウト', destroy_user_session_path, method: :delete %>
    <% end %>
  </nav>
</header>

<% provide(:title, 'All users') %>
<h1><%= t('devise.users.list') %></h1>
 
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user %>
    </li>
  <% end %>
</ul>

<%= paginate @users %>
